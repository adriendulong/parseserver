<body onload ="initPhotoInfo ()" >

	<div class="loading-div">
	
		
		<img class="loader-img-center" src="http://www.woovent.com/app/img/loading-marmotte.png" alt="Woovent Photo" height="188" style="margin-bottom : 50px;" />
		
		<div class="loading"></div>
		
		<p class="main-loading-title"> <script> document.write(photoview_waiting_title);</script></p>
		<p class="sub-loading-title"> <script> document.write(photoview_waiting_subtitle);</script></p>
	
	</div>
	
	<div class="row photo-container" style="height : 100% ; visibility : hidden;">
	    	<div class="col-xs-12 col-sm-12 col-md-8 col-lg-8 col-img-full" >
		    	<div id="img-full-div">
		    		<p class="photo-line-height">
		    		<img src="<%= photoSrc %>" alt="Woovent Photo" height=""/>
		    		</p>
		    	</div>
	    	
	    	</div>
	    	
	    	<div class="col-xs-12 col-sm-12 col-md-4 col-lg-4 col-comment-full" >
	    	
	    	
				<div id="comment-full-div">
				
					<div class="comment-loading-div" style="display : none;">
			
						<img class="loader-img-center" src="http://www.woovent.com/app/img/loading-marmotte.png" alt="Woovent Photo" height="120" style="margin-bottom : 50px;" />
						
						<div class="comment-loading"></div>
						
						<p class="comment-loading-title"> 
							<script>document.write(photoview_comment_sending);</script> 
						</p>
					
					</div>

					<div class="owner-div" >
						<img src="<%= userProfilPicUrl %>" alt="Woovent Progil pic Photo" height="70px"/>
						
						<div class="owner-div-description"> 
							<p>
								<span class="owner-div-userName"> <%= userName %> </span> <br />
								<span class="owner-div-Date"> 
									<script> 									
										var userLang = navigator.language || navigator.userLanguage; 
										var createdTimeDate = new Date(createdTime);

										if (userLang == "fr"){
										
												//on converti la date francaise
												var eventStartDay = createdTimeDate.getDay();
												var eventStartMonth= createdTimeDate.getMonth();
												var frenchEventBeginDay = getFrenchDay(eventStartDay);
												var frenchEventbeginMonth = getFrenchMonth(eventStartMonth);
												var eventStartDayFrench = frenchEventBeginDay + " " + createdTimeDate.getDate() +" "+ frenchEventbeginMonth;
												var eventEndHourFrench = createdTimeDate.getHours() + " heures ";
												var eventStartDate = eventStartDayFrench + " à " + eventEndHourFrench;
											
											}else {
											
												//on converti la date  anglaise
												var eventStartDay = createdTimeDate.getDay();
												var eventStartMonth= createdTimeDate.getMonth();
												var englishEventBeginDay = getEnglishDay(eventStartDay);
												var englishEventbeginMonth = getEnglishMonth(eventStartMonth);
												var eventStartDayEnglish = englishEventBeginDay + ", " + englishEventbeginMonth + " " + createdTimeDate.getDate();
												
												if(createdTimeDate.getHours() > 12) {
													var eventStartHourEnglishNew = createdTimeDate.getHours() - 12;
													var eventStartHourEnglish = eventStartHourEnglishNew + " pm ";
												} else {
													var eventStartHourEnglish = createdTimeDate.getHours() + " am ";
												}
												
												var eventStartDate = eventStartDayEnglish + " at " + eventStartHourEnglish;
												
											}	
											
											document.write(eventStartDate);
									
											
									</script> 
								</span>
							</p>
						</div>
					
					
					</div>

					<div class="like-div">
						<p>
							<i class="icon-heart" style="color : grey;margin-right : 5px;"></i>
							<span class="like-div-likers"><script> document.write(photoview_nolike);</script></span>
						</p>
					</div>
					
					
					<div class="comment-div">
						<!--
						<div class="comment-content" id="commentId">
							<div class="userCommentProfilPic">
								<img src="<%= userProfilPicUrl %>" alt="Woovent Progil pic Photo" height="45px"/>
							</div>
	
							<div class="comment-text">
								<span class="commentUserName"> Mon nom </span><br />
								<span class="comment-text-content"> Mon commentaire de ouf </span>
							</div>
						</div>
						-->
						
					</div>
					
					<div class="post-div">
						<div class="post-div-content">
						
							<a class=" like-button-link"  onclick="likeButtonPicture('<%= idPhoto %>')"  >
								<i class="icon-heart like-button-picto" style="color : grey; text-decoration: none;"></i>
							</a>
							<div class="input-container">
		<script>
		 var userLang = navigator.language || navigator.userLanguage; 
		 if (userLang == "fr"){
		 	document.write('<input type="text" class="post-div-input" placeholder="Laisser un commentaire..." maxlength="200">');
		 }else {
			 document.write('<input type="text" class="post-div-input" placeholder="Leave a comment..." maxlength="200">');
		 }
				
		</script>
								
							</div>
							
						
						</div>
